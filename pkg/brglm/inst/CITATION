citHeader("To cite package 'brglm' in publications use:")

citEntry(entry="Manual",
    title = {"brglm: Bias reduction in binary-response GLMs"},
    author = personList(as.person("Ioannis Kosmidis")),
    year = {2007},
    note = {"R package version 0.5-6"},
    url = {"http://www.ucl.ac.uk/~ucakiko/software.html"},
    textVersion = paste(
      "Ioannis Kosmidis (2007).",
      "brglm: Bias reduction in binary-response GLMs.",
      "http://www.ucl.ac.uk/~ucakiko/software.html")
)


